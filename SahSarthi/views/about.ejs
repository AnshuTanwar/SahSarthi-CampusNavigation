        <!-- About -->
        <section class="row" id="tmAbout" data-aos="fade-up">
          <header class="col-12 tm-about-header" data-aos="fade-down">
            <h2 class="text-uppercase text-center text-dark tm-about-page-title">About this app</h2>
            <hr class="tm-about-title-hr">
          </header>
          <div class="col-lg-4" data-aos="fade-up" data-aos-delay="100">
            <div class="tm-bg-black-transparent tm-about-box">
              <div class="tm-about-number-container">0.1</div>              
              <h3 class="tm-about-name">Purpose</h3>
              <p class="tm-about-description">
                SHSARTHI makes campus navigation easy and efficient for students, staff, and visitors. The app provides accurate directions to buildings, classrooms, and essential facilities, helping you save time and focus on what’s important.
              </p>
            </div>
          </div>

          <div class="col-lg-4" data-aos="fade-up" data-aos-delay="200">
